class MyCircularQueue {
    private int[] queue;
    private int cap;
    private int head;
    private int size;

    public MyCircularQueue(int k) {
        cap = k;
        queue = new int[k];
        head = 0;
        size = 0;
    }
    
    public boolean enQueue(int value) {
        if (isFull()) return false;
        queue[(head + size) % cap] = value;
        size++;
        return true;
    }
    
    public boolean deQueue() {
        if (isEmpty()) return false;
        head = (head + 1) % cap;
        size--;
        return true;
    }
    
    public int Front() {
        return isEmpty() ? -1 : queue[head];
    }
    
    public int Rear() {
        if (isEmpty()) return -1;
        return queue[(head + size - 1) % cap];
    }
    
    public boolean isEmpty() {
        return size == 0;
    }
    
    public boolean isFull() {
        return size == cap;
    }
}

